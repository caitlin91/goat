author = {Dickson, Victoria and Hall-Lew, Lauren},
doi = {10.1177/0075424217718024},
file = {:C$\backslash$:/Users/Caitlin/OneDrive/Documents/Mendeley docs/DicksonandHallLew{\_}2017.pdf:pdf},
journal = {Journal of English Linguistics},
number = {3},
pages = {229--259},
title = {{Class, Gender, and Rhoticity: The Social Stratification of Non-Prevocalic /r/ in Edinburgh Speech}},
url = {https://doi.org/10.1177/0075424217718024},
volume = {45},
year = {2017}
}
