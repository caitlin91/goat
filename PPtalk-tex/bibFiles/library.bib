@article{Hofwegen2010,
abstract = { ABSTRACT In the wake of numerous analyses of vowels in African American
English (AAE), this study examines acoustically the phonetic production
of a consonant---the word-initial lateral /l/---across several generations
of speakers from a long-standing African American community in central
North Carolina. The results of the study show that /l/ is darker
in younger AAE speakers than in older ones, independent of phonetic
context. Comparisons with ex-slave recordings suggest that a light
variant of /l/ may be a substrate feature of AAE that has changed
in recent decades. Additional comparisons with regional European
Americans suggest that the darkening may be due to convergence with
majority American English dialects. },
annote = {- F1 and F2 measurements were taken at the midpoint of each /l/ token. 
- For normalization, all formant measurements were converted to Bark using Traunm{\"{u}}ller's (1990) formula and the difference between formants was computed. Bark-converted F2−F1, the measure of velarization, is referred to as Z2−Z1.
- Phonetic effects of duration were gauged using time measurements at both the onset and the offset of each /l/ token. 
- For coarticulation effects, an F2 measurement was taken 30 msec into the following vowel.
- F2 values at 30 milliseconds into the following vowel were converted to Bark using Traunm{\"{u}}ller's (1990) formula, and measures of coarticulation were normalized as the arithmetic difference between Z2 (at /l/ midpoint) and Z2 (at 30 msec into the following vowel).
- Formant measures were extracted with PRAAT's defaults set to .025 seconds (window length), 30.0 dB (dynamic range), and 1.0 mm (dot size), with maximum formant set to 4000 Hz.},
author = {van Hofwegen, Janneke},
journal = {Language Variation and Change},
number = {03},
pages = {373--396},
title = {{Apparent-time evolution of /l/ in one {\{}African American{\}} community}},
volume = {22},
year = {2010}
}
