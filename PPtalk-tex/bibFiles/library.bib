issn = {0001-4966},
journal = {The Journal of the Acoustical Society of America},
number = {6},
pages = {3782--3790},
pmid = {8655809},
title = {{The processing of duration and intensity cues to prominence}},
url = {http://asa.scitation.org/doi/10.1121/1.414995},
volume = {99},
year = {1996}
}
