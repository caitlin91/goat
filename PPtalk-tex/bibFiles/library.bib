@article{Tomlinson2014,
abstract = {According to the rhythm rule in metrical phonology, a word's stress alternates from the second syllable to the first when followed by a word with first syllable stress, a situation also known as a stress clash. For example, the secondsyllable stressed word sixTEEN will be produced as SIXteen in the phrase SIXteen CANdles. Using pseudo-words in different rhythmic contexts, we demonstrate that stress clash resolution has a strong perceptual component and can be an auditory illusion. In Experiment 1, participants were more likely to misattribute stress in a clash condition (sixTEEN heard as SIXteen in sixTEEN CANdles) than in isolation (sixTEEN) or in a non-clash condition (sixTEEN caNALS). In Experiment 2, we failed to find support for the hypothesis that nuclear stress drove these ratings. We discuss these findings in light of various theories and mechanisms for parsing spontaneous speech.},
author = {Tomlinson, John M. and Liu, Qiang and {Fox Tree}, Jean E.},
doi = {10.1080/01690965.2013.813561},
file = {:C$\backslash$:/Users/Caitlin/OneDrive/Documents/Mendeley docs/Tomlinsonetal{\_}2014.pdf:pdf},
issn = {23273801},
journal = {Language, Cognition and Neuroscience},
keywords = {Auditory illusion,Beat clash,Nuclear stress,Rhythm,Speech perception},
number = {9},
pages = {1046--1058},
publisher = {Taylor {\&} Francis},
title = {{The perceptual nature of stress shifts}},
