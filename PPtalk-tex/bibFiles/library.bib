file = {:C$\backslash$:/Users/Caitlin/OneDrive/Documents/Mendeley docs/Fabriciusetal{\_}2009.pdf:pdf;:C$\backslash$:/Users/Caitlin/OneDrive/Documents/Mendeley docs/Fabriciusetal{\_}2009.pdf:pdf},
isbn = {0954-3945},
issn = {0954-3945},
journal = {Language Variation and Change},
