@article{Guy1991,
abstract = {Variationist treatments of phonological processes typically provide precise quantitative accounts of the effects of conditioning environmental$\backslash$nfactors on the occurrence of the process, and these effects have$\backslash$nbeen shown to be robust for several well-studied processes. But comparable$\backslash$nprecision in theoretical explanation is usually elusive, at the current$\backslash$nstate of the discipline. That is, the analystis usually unable to$\backslash$nsay why the parameters should have the particularvalues that they$\backslash$ndo, although one canoften explain relative ordering of environments.$\backslash$nThis article attempts to give a precise explanation---in the form$\backslash$nof a quantitative theoretical prediction---of one robust quantitative$\backslash$nobservation about English phonology. The reduction of final consonant$\backslash$nclusters (often called -t,d deletion) is well-known to be conditioned$\backslash$nby the morphological structure of a target word. Deletion applies$\backslash$nmore in monomorphemic words (e.g., mist) than in inflected words$\backslash$n(e.g., missed). In the theory of lexical phonology, these classes$\backslash$nof words are differentiated by derivational history, acquiring their$\backslash$nfinal clusters at different levels of the morphology. The theory$\backslash$nfurther postulates that rules may apply at more than one level of$\backslash$nthe derivation. If -t, d deletion is treated as a variable rule with$\backslash$na fixed rate of application (Po) in a phonology with this architecture,$\backslash$nthen higher rates of application in underived forms (where the final$\backslash$ncluster is present underlyingly and throughout the derivation) are$\backslash$na consequence of multiple exposures to the deletion rule, whereas$\backslash$ninflected forms (which only meet the structural description of the$\backslash$nrule late in the derivation) have fewer exposures and lower cumulative$\backslash$ndeletion.This further allows a precise quantitative prediction concerning$\backslash$nsurface deletion rates in the different morphological categories.$\backslash$nThey should be related as an exponential function of Po, depending$\backslash$non the number of exposures to the rule. The.prediction is empirically$\backslash$nverified in a study of -t,d deletion in seven English speakers.},
annote = {NULL},
author = {Guy, Gregory R.},
doi = {10.1017/S0954394500000429},
file = {:C$\backslash$:/Users/Caitlin/OneDrive/Documents/Mendeley docs/Guy{\_}1991.pdf:pdf},
issn = {0954-3945},
journal = {Language Variation and Change},
pages = {1},
publisher = {Newcastle University},
title = {{Explanation in variable phonology: An exponential model of morphological constraints}},
volume = {3},
year = {1991}
}
