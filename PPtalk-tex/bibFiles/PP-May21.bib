@article{Guy1991,
	title = {Explanation in variable phonology: {An} exponential model of morphological constraints},
	volume = {3},
	issn = {0954-3945},
	doi = {10.1017/S0954394500000429},
	abstract = {Variationist treatments of phonological processes typically provide precise quantitative accounts of the effects of conditioning environmental{\textbackslash}nfactors on the occurrence of the process, and these effects have{\textbackslash}nbeen shown to be robust for several well-studied processes. But comparable{\textbackslash}nprecision in theoretical explanation is usually elusive, at the current{\textbackslash}nstate of the discipline. That is, the analystis usually unable to{\textbackslash}nsay why the parameters should have the particularvalues that they{\textbackslash}ndo, although one canoften explain relative ordering of environments.{\textbackslash}nThis article attempts to give a precise explanation---in the form{\textbackslash}nof a quantitative theoretical prediction---of one robust quantitative{\textbackslash}nobservation about English phonology. The reduction of final consonant{\textbackslash}nclusters (often called -t,d deletion) is well-known to be conditioned{\textbackslash}nby the morphological structure of a target word. Deletion applies{\textbackslash}nmore in monomorphemic words (e.g., mist) than in inflected words{\textbackslash}n(e.g., missed). In the theory of lexical phonology, these classes{\textbackslash}nof words are differentiated by derivational history, acquiring their{\textbackslash}nfinal clusters at different levels of the morphology. The theory{\textbackslash}nfurther postulates that rules may apply at more than one level of{\textbackslash}nthe derivation. If -t, d deletion is treated as a variable rule with{\textbackslash}na fixed rate of application (Po) in a phonology with this architecture,{\textbackslash}nthen higher rates of application in underived forms (where the final{\textbackslash}ncluster is present underlyingly and throughout the derivation) are{\textbackslash}na consequence of multiple exposures to the deletion rule, whereas{\textbackslash}ninflected forms (which only meet the structural description of the{\textbackslash}nrule late in the derivation) have fewer exposures and lower cumulative{\textbackslash}ndeletion.This further allows a precise quantitative prediction concerning{\textbackslash}nsurface deletion rates in the different morphological categories.{\textbackslash}nThey should be related as an exponential function of Po, depending{\textbackslash}non the number of exposures to the rule. The.prediction is empirically{\textbackslash}nverified in a study of -t,d deletion in seven English speakers.},
	number = {1},
	journal = {Language Variation and Change},
	author = {Guy, Gregory R.},
	year = {1991},
	keywords = {★},
	pages = {1--22},
	annote = {From Duplicate 1 (Explanation in variable phonology: An exponential model of morphological constraints - Guy, Gregory R.)

