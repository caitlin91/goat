@article{johnson_university_2015,
	title = {University of {Pennsylvania} {Working} {Papers} in {Linguistics} {Partial} {Mergers} and {Near}-{Distinctions}: {Stylistic} {Layering} in {Dialect} {Acquisition} {Partial} {Mergers} and {Near}-{Distinctions}: {Stylistic} {Layering} in {Dialect} {Acquisition} {Partial} {Mergers} and {Near}-{Distinction}},
	volume = {21},
	url = {http://repository.upenn.edu/pwpl/vol21/iss2/13%0Ahttp://repository.upenn.edu/pwpl/},
	abstract = {Non-mobile individuals living in a community undergoing a linguistic change usually display a pattern of variation where citation styles are more advanced than naturalistic styles with respect to the change in progress. This suggests that speakers' intentions or norms, while they may shift only gradually, can do so more easily than their productions. Two groups of 'mobile' speakers—adults who grew up in one dialect area and moved to another, and children who acquired one dialect at home and another in the community—showed the opposite pattern: spontaneous speech approximated the dialect of the second community more closely than did word-list productions. Analysis of these speakers' low back vowels (the LOT and THOUGHT word classes) also suggest that most adults can at least partially learn a vowel distinction as well as a merger, though children are able to do both things more quickly and completely.},
	number = {2},
	journal = {Selected from New Ways of Analyzing Variation (NWAV) 43},
	author = {Johnson, Daniel Ezra and Nycz, Jennifer},
	year = {2015},
	pages = {107--117},
}

