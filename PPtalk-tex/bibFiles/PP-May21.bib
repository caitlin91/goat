@article{Rubach1996a,
	title = {Shortening and ambisyllabicity in {English}},
	volume = {13},
	issn = {0952-6757},
	doi = {10.1017/S0952675700002104},
	abstract = {Based on a typology of examples of vowel shortening in British English Received Pronunciation (RP), the cluster shortening proposal of Moira Yip (1987 [see abstract 8803112]) \& the syllabic shortening proposal defended by Scott Myers (1987 [see abstract 8803076]) are critiqued \& found inadequate, in particular as mirror images, to explain the contradictory data types national vs notional \& tyrannize vs polarize. An alternative proposal analyzes the initial vowel of suffixes that condition shortening as a floating matrix, ie, a defective segment invisible to syllabification \& vocalized after syllabic shortening. Shortening is therefore conditioned by closed syllables. Formalizations in skeletal, moraic, \& lexical phonology models are discussed. Evidence of American English flapping \& RP tapping, dark /l/, \& syllabic consonants is adduced to show that ambisyllabification is necessary to describe English \& is independent from shortening. Resyllabification accounts of the same data are examined \& rejected. 66 References. J. Hitchcock},
	number = {02},
	journal = {Phonology},
	author = {Rubach, Jerzy},
	year = {1996},
	note = {Publisher: Newcastle University
ISBN: 1469-8188},
	pages = {197--237},
