month = {jan},
number = {2},
pages = {315--328},
publisher = {De Gruyter Mouton},
title = {{The "up" corpus: A corpus of speech samples across adulthood}},
url = {https://www.degruyter.com/view/j/cllt.2014.10.issue-2/cllt-2013-0023/cllt-2013-0023.xml},
volume = {10},
