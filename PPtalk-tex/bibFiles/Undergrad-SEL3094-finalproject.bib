pages = {211--237},
publisher = {Newcastle University},
title = {{Weak vowels in modern RP: An acoustic study of happ$\backslash$scs{\{}y{\}}-tensing and $\backslash$scs{\{}kit{\}}/schwa shift}},
volume = {14},
year = {2002}
}
